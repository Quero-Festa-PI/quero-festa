
<% include partials/header %>

<body>
    <div class="page">
        <% include partials/menu-logado %>
        
        <!-- CONTEÚDO PRINCIPAL -->
        <div class="container">      
          <div class="row ">
            <!-- MENU -->
            <% include partials/menu-horizontal %> 
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
              <div class="homebuilder clearfix"> 
                <div class="tm-container">
                                   
                  <!-- CONTAINER FORMULARIO DADOS PESSOAIS-->
                  <div class="tm-container">
                    <div class="tm-inner">
                      <div class="row row-level-1">                      
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                          <div class="row-inner clearfix">
                              <div class="col-inner d-flex justify-content-around">
                                <div id="content" class="col-sm-12 col-md-12 col-lg-12">
                                  
                                  <!-- FORMULARIO -->
                                  
                                  <form class="bg-light rounded col-12 shadow p-3 mb-5 bg-white rounded"> 
                                    <div class="form-group row">
                                      
                                      <label for="dadosPessoais" class="col-8 col-form-label">
                                        <h3>Dados Pessoais</h3>
                                      </label>
                                      <a class='btn' href='/painel-cliente'>
                                        <input type='submit' class='btn-cheio btn text-left' action='' value="Enviar Dados">
                                      </a>
                                      
                                    </div>
                                        <div class="row">
                                          <div class="col-8">

                                            <!-- NOME COMPLETO-->
                                           <div class="form-group row">
                                             <label for="inputNome" class="col-sm-3 col-form-label">Nome Completo</label>
                                             <div class="col-sm-5">
                                               <input type="text" class="form-control" id="inputNome">
                                             </div>
                                             
                                           </div>
                                           <!-- DATA NASCIMENTO -->
                                           <div class="form-group row">
                                             <label for="inputData" class="col-sm-3 col-form-label">Data de Nascimento</label>
                                             <div class="col-sm-5">
                                               <input type="date" class="form-control" id="inputData">
                                             </div>
                                           </div>
                                           <!-- EMAIL -->
                                           <div class="form-group row">
                                             <label for="inputEmail" class="col-sm-3 col-form-label">Email</label>
                                             <div class="col-sm-5">
                                               <input type="email" class="form-control" id="inputEmail">
                                             </div>
                                           </div>
                                           <!-- SEXO -->
                                           <fieldset class="form-group">
                                             <div class="row">
                                               <legend class="col-form-label col-sm-3 pt-0">Sexo</legend>
                                               <div class="form-check form-check-inline">
                                                 <input class="form-check-input" type="radio" name="radioSexo" id="radioSexoF" value="opcao1">
                                                 <label class="form-check-label" for="radioSexoF">Feminino</label>
                                               </div>
                                               <div class="form-check form-check-inline">
                                                 <input class="form-check-input" type="radio" name="radioSexo" id="radioSexoM" value="opcao2">
                                                 <label class="form-check-label" for="radioSexoM">Masculino</label>
                                   
                                               </div>
                                             </div>
                                           </fieldset>  
                                           
                                          </div>

                                          <div class="col-4">

                                            <!-- FOTO DE PERFIL -->
                                <div class="row">        
                                  <div class="col">
                                    <div class="d-flex flex-row-reverse ">        
                                      <div class="col-8">
                                        <div class="d-flex justify-content-around bd-highlight">
                                          <div class="p-2 bd-highlight">
                                            <form id="form-img" class="bg-light rounded col-sm-12 shadow ml-6 bg-white rounded d-inline-flex  p-2">
                                              <div class="form-group d-inline ml-6">
                                                <label for="customFile">
                                                  <img src="/images/sem-foto2.png" class="shadow mb-1 ml-1">                                    
                                                  <input type="file" class="custom-file-input" id="customFile">
                                                </label>
                                              </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>
                                <!-- FIM DA FOTO DE PERFIL -->
                                          </div>
                                    </form>
  
                                   
  
                                    <!-- FIM FORMULARIO -->

                                  </div>
                                </div> 
                              </div>                                  
                          </div>
                        </div>                        
                      </div>
                    </div>
                  </div>
                  <!-- FIM CONTAINER -->

                  <!-- CONTAINER FORMULARIO ENDEREÇO-->
                  <div class="tm-container">
                    <div class="tm-inner">
                      <div class="row row-level-1">                      
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                          <div class="row-inner clearfix">
                              <div class="col-inner d-flex justify-content-around">
                                <div id="content" class="col-sm-12 col-md-12 col-lg-12">
                                  <!-- FORMULARIO -->
                                  <form class="bg-light rounded col-sm-12 shadow p-3 mb-5 bg-white rounded"> 
                                    <div class="form-group row">
                                      <label for="dadosPessoais" class="col-sm-4 col-form-label">
                                        <h3>Endereço</h3>
                                      </label>
                                    </div>
                                    <!-- CEP-->
                                    <div class="form-group row">
                                      <label for="inputRua" class="col-sm-3 col-form-label">CEP</label>
                                      <div class="col-sm-4">
                                        <input type="text" class="form-control" id="inputCep">
                                      </div>
                                      
                                    </div>
                                    <!-- RUA -->
                                    <div class="form-group row">
                                      <label for="inputRua" class="col-sm-3 col-form-label">Rua</label>
                                      <div class="col-sm-9">
                                        <input type="text" class="form-control" id="inputRua">
                                      </div>
                                    </div>
                                    <!-- CIDADE -->
                                    <div class="form-group row">
                                      <label for="inputCidade" class="col-sm-3 col-form-label">Cidade</label>
                                      <div class="col-sm-9">
                                        <input type="text" class="form-control" id="inputCidade">
                                      </div>
                                    </div>                                    
                                        <div class="form-group row">
                                          <label for="inputEstado" class="col-sm-3 col-form-label">Estado</label>
                                          <div class="col-sm-4">
                                            <select id="inputEstado" class="form-control">
                                              <option selected>Escolher...</option>
                                              <option value="1">São Paulo</option>
                                              <option value="2">Rio de Janeiro</option>
                                              <option value="3">Minas Gerais</option>
                                              <option value="4">Rio Grande do Sul</option>
                                            </select>
                                          </div>
                                          
                                        </div>
                                        
                                        
                                  </form>
                                  <!-- FIM FORMULARIO -->
                                </div> 
                              </div>                                  
                          </div>
                        </div>                        
                      </div>
                    </div>
                  </div>
                  <!-- FIM CONTAINER -->
                    <div class="tm-container">
                        <div class="tm-inner">
                            <div class="row row-level-1">                      
                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                    <div class="row-inner clearfix">
                                        <div class="col-inner d-flex justify-content-around">
                                            <div id="content" class="col-sm-12 col-md-12 col-lg-12">
                                                    <!-- FORMULARIO -->
                                                    <form class="bg-light rounded col-sm-12 shadow p-3 mb-5 bg-white rounded"> 
                                                      <div class="form-group row">
                                                        <label for="dadosPessoais" class="col-sm-4 col-form-label">
                                                        <h3>Mudar Senha</h3>
                                                        </label>
                                                      </div>
                                                      <!-- Senha-->
                                                      <div class="form-group row">
                                                        <label for="inputSenha" class="col-sm-3 col-form-label">Senha Atual:</label>
                                                        <div class="col-sm-4">
                                                            <input type="password" class="form-control" id="inputSenha">
                                                        </div>                                        
                                                      </div>
                                                      <!-- Nova Senha -->
                                                      <div class="form-group row">
                                                        <label for="inputNovaSenha" class="col-sm-3 col-form-label">Nova Senha:</label>
                                                        <div class="col-sm-4">
                                                          <input type="password" class="form-control" id="inputNovaSenha">
                                                        </div>
                                                      </div>
                                                      <!-- Confirmar Senha -->
                                                      <div class="form-group row">
                                                        <label for="inputCorfirmarSenha" class="col-sm-3 col-form-label">Confirmar Senha:</label>
                                                        <div class="col-sm-4">
                                                          <input type="password" class="form-control" id="inputConfirmarSenha">
                                                        </div>
                                                      </div>     
                                                      </form>                       
                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                
                
                
                

                
                </div>
              </div>
            </div>

          </div>
        </div>
    </div>        
</body>