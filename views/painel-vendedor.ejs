                    </div>
                </div>
    
    
                <div class='painel-graf mx-5'>
                    <!-- ### Card Clientes Recentes -->
                    <div class="card painel-dash border-left-primary shadow col-6">
                        <div class="card-body">
                            <h5>Clientes</h5>
                            <h6 class='text-muted'>Em Breve...</h6>
                            <br><br><br><br>                                
                        </div>
                    </div>
    
                    <!-- ### Card Avaliacoes -->
                    <div class="card painel-dash border-left-primary shadow col-6">
                        <div class="card-body">
                            <h5>Avaliações</h5>
                            <h6 class='text-muted'>Em Breve...</h6>
                            <br><br><br><br>                                
                        </div>
                    </div>
                </div>
    
                <!-- ### CARD DE MUDAR PERFIL -->
                <div class="card painel-dash border-left-primary shadow mx-5">
                    <a href='../painel-cliente'>
                        <button class="btn btn-cheio btn-small col-12 text-left">
                            Alterar para Perfil Cliente </button>
                    </a>
                </div>
            </div>            
        </div>
    </div>  

    </div>
  </div>
  <br><br>
  <% include partials/footer-alt %>
